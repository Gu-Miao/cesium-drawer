{
  "name": "cesium-toolkit",
  "version": "0.3.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gu-Miao/cesium-toolkit.git"
  },
  "keywords": [
    "cesium",
    "esm",
    "typescript"
  ],
  "author": "GuTianhuang",
  "bugs": {
    "url": "https://github.com/Gu-Miao/cesium-toolkit/issues"
  },
  "homepage": "https://github.com/Gu-Miao/cesium-toolkit#readme",
  "global": "CesiumToolkit",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "files": [
    "package.json",
    "README.md",
    "README_zh.md",
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "serve": "wait-on dist/index.js && live-server --open=test --watch=test,dist",
    "watch": "rollup -c -w",
    "start": "concurrently \"npm run watch\" \"npm run serve\"",
    "build": "cross-env NODE_ENV=production rollup -c",
    "lint": "eslint src --max-warnings 0",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@swc/core": "^1.2.150",
    "@swc/helpers": "^0.3.6",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "cesium": "^1.93.0",
    "concurrently": "^7.1.0",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "live-server": "^1.2.1",
    "prettier": "^2.4.1",
    "rollup": "^2.57.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^2.0.5",
    "typescript": "^4.4.3",
    "wait-on": "^6.0.1"
  },
  "peerDependencies": {
    "cesium": "*"
  }
}
