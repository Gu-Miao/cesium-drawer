{
  "name": "cesium-toolkit",
  "version": "0.4.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gu-Miao/cesium-toolkit.git"
  },
  "keywords": [
    "cesium",
    "esm",
    "typescript"
  ],
  "author": "GuTianhuang",
  "bugs": {
    "url": "https://github.com/Gu-Miao/cesium-toolkit/issues"
  },
  "homepage": "https://github.com/Gu-Miao/cesium-toolkit#readme",
  "global": "CesiumToolkit",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "packageManager": "yarn@3.2.1",
  "files": [
    "package.json",
    "README.md",
    "README_zh.md",
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "serve": "wait-on dist/index.js && live-server --open=test --watch=test,dist",
    "watch": "rollup -c -w",
    "start": "concurrently \"npm run watch\" \"npm run serve\"",
    "build": "cross-env NODE_ENV=production rollup -c",
    "lint": "eslint src --max-warnings 0",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@swc/core": "^1.2.210",
    "@swc/helpers": "^0.4.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "cesium": "^1.95.0",
    "concurrently": "^7.2.2",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "live-server": "^1.2.2",
    "prettier": "^2.7.1",
    "rollup": "^2.75.7",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^3.0.2",
    "typescript": "^4.7.4",
    "wait-on": "^6.0.1"
  },
  "peerDependencies": {
    "cesium": "*"
  }
}
